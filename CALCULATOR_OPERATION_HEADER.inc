;OPERACAO DE SUBTRACAO    
OPERATOR_SUB:
   MOVF INPUT_U1 , W
   SUBLW D'0'
   BTFSC STATUS, Z
   CALL REAJUST
   MOVF INPUT_U2,W
   SUBWF INPUT_U1,W
   MOVWF UNIDADE
   MOVF INPUT_D2,W
   SUBWF INPUT_D1,W
   MOVWF DEZENA
   RETURN
CASE_0:
   MOVF INPUT_U2,W
   MOVWF UNIDADE
   MOVF INPUT_D2,W
   MOVWF DEZENA
   RETURN
REAJUST:
   MOVF INPUT_D1 , W
   SUBLW D'0'
   BTFSC STATUS, Z
   GOTO CASE_0
   MOVF INPUT_D1 , W
   SUBLW D'1'
   MOVWF INPUT_D1
   MOVF INPUT_U1,W
   ADDLW D'10'
   MOVWF INPUT_U1
   RETURN
 
   
   
